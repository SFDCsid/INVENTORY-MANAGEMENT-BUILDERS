                @page {
                size: A4;
                @bottom-center {          
                content: "Page " counter(page) " of " counter(pages);                
                }
                }
                body{
                margin:10px;
                padding:0;            
                font-family:"Google Sans", Roboto, arial, sans-serif !important;
                font-family: 'cambria';
                }
                table{
                width:100%;
                border-collapse:collapse;
                font-size: 12px;
                }
                table tr th,td{
                border:1px solid #000;
                
                font-size:12px;
                padding: 3px;
                }                
                .table1{
                width:100%;
                border-collapse:collapse;
                font-size: 12px;
                padding: 3px;
                }
                .table1 tr th{
                border:1px solid #000;
                text-align:center;
                font-size:12px;
                padding: 3px;
                background-color:#9CC4E4;
                }
