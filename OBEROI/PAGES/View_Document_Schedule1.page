<apex:page applyBodyTag="false" controller="ViewDocumentSync"  showHeader="false" sidebar="false" standardStylesheets="false" applyHtmlTag="true" renderAs="{!renderProp}"  docType="HTML-5.0"> <!--contentType="application/msword#filename.doc"  --->
    
    <head>
        <style type="text/css" media="print">
            @page {
            size: A4;
            margin:35px;
            //border:1px solid #000;
            
            padding:10px 12px;
            @bottom-center {          
            content: "Page " counter(page) " of " counter(pages);                
            }
            }
            @page {
            size: 8.87in 10.0in;
            }
            body{
            margin:0;
            padding:0;            
            //font-family:"Google Sans", Roboto, arial, sans-serif !important;
            //font-family: 'cambria';
            font-size:14px;
            }
            .table1{
            width:100%;
            border-collapse:collapse;
            } 
            .table1 tr td{
            padding:5px 6px;
            }
            .table3{
            width:100%;
            border-collapse:collapse;
            } 
            .table3 tr td{
            padding:6px 10px;
            }
            .table2{
            width:100%;
            border-collapse:collapse;
            }
            .table2 tr th{
            border:1px solid #000;
            font-size:11px;
            padding:3px 5px;
            background:#ddd;
            }
            .table2 tr td{
            font-size:11px;
            border:1px solid #000;
            padding:3px 5px;
            }
            
            @page {
            margin-bottom: 115px;
            margin-top: 80px;
            
            @top-center {
            content: element(header,first-except);
            }
            @bottom-center {
            content: element(footer,first-except);
            }
            }            
            .header{
            position: running(header);
            display:block;
            margin-top:30px;
            }
            .footer {
            display:block;
            position: running(footer);
            z-index:-999;
            text-align:center;
            }
            .pagenumber:before {
            content: counter(page);
            }
            .pagecount:before {
            content: counter(pages);
            }
            
            
        </style>
    </head>
